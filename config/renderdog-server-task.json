{
   "family" : "renderdog",
   "containerDefinitions" : [
      {
         "environment" : [
            {
               "value" : "<SECRET_STUFF>",
               "name" : "AWS_ACCESS_KEY_ID"
            },
            {
               "value" : "<SUPER_SECRET_STUFF",
               "name" : "AWS_SECRET_ACCESS_KEY"
            },
            {
               "value" : "us-west-2",
               "name" : "AWS_REGION"
            }
         ],
         "memory" : 384,
         "name" : "renderdog",
         "image" : "jamesonwilliams/renderdog",
         "essential" : true,
         "portMappings" : [
            {
               "hostPort" : 8000,
               "containerPort" : 8000,
               "protocol" : "tcp"
            }
         ]
      }
   ]
}
